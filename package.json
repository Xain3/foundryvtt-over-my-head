{
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@rollup/plugin-json": "^6.1.0",
    "@vitest/coverage-v8": "^2.1.4",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "cypress": "^15.0.0",
    "eslint": "^8.57.1",
    "jsdom": "^26.1.0",
    "jsonc-parser": "^3.3.1",
    "rewire": "^7.0.0",
    "vite": "^7.1.1",
    "vitest": "^2.1.4"
  },
  "scripts": {
    "dev": "node ./scripts/dev/buildAndDeploy.mjs",
    "build": "vite build",
    "build:prod": "NODE_ENV=production vite build",
    "prerelease": "npm run test && npm run build:prod",
    "release": "npm run prerelease && git add dist/ && git commit -m 'Build artifacts'",
    "test": "vitest run --coverage",
    "test:unit": "vitest run --coverage --exclude='tests/integration/**' --exclude='**/*.int.test.mjs'",
    "test:integration": "vitest run tests/integration/",
    "test:setup": "vitest run tests/setup/",
    "test:watch": "vitest",
    "test:functional": "echo 'Functional tests not configured' && exit 0",
    "test:performance": "echo 'Performance tests not configured' && exit 0",
    "validate:container-config": "node ./scripts/validate-container-config.mjs"
  },
  "dependencies": {
    "csslint": "^1.0.5",
    "dayjs": "^1.11.13",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "validator": "^13.15.0",
    "zod": "^3.24.4"
  }
}
