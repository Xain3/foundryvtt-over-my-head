{
  "$schema": "container-config.schema.json",

  "systems": {
    "worldbuilding": {
      "name": "Simple Worldbuilding System",
      "manifest": "https://raw.githubusercontent.com/foundryvtt/worldbuilding/HEAD/system.json",
      "install_at_startup": true
    }
  },
  "modules": {
    "foundryvtt_over_my_head": {
      "name": "FoundryVTT Over My Head",
      "manifest": "https://raw.githubusercontent.com/Xain3/foundryvtt-over-my-head/HEAD/module.json",
      "install_at_startup": false
    }
  },
  "worlds": {
    "over_my_head_dev_world": {
      "name": "Over My Head Dev World",
      "manifest": "",
      "path": "/host/shared/worlds/test-world",
      "install_at_startup": false
    }
  },

  "composition": {
    "builder": {
      "enabled": true,
      "image": "node:20-alpine"
    },
    "baseImage": "felddy/foundryvtt",
    "user": "0:0",
    "version_params": {
      "name": "foundry-v{version}",
      "tag": "release",
      "port": "300{version}",
      "versionDir": "v{version}",
      "envSuffix": "v{version}"
    }
  },

  "versions": {
    "13": {
      "supported": true,
      "install": {
        "systems": {
          "worldbuilding": {}
        },
        "modules": {
          "foundryvtt_over_my_head": {}
        },
        "worlds": {
          "over_my_head_dev_world": {}
        }
      }
    },

    "12": {
      "supported": true,
      "install": {
        "systems": {
          "worldbuilding": {}
        },
        "modules": {
          "foundryvtt_over_my_head": {}
        },
        "worlds": {
          "over_my_head_dev_world": {}
        }
      }
    },

    "11": {
      "supported": true,
      "install": {
        "systems": {
          "worldbuilding": {}
        },
        "modules": {
          "foundryvtt_over_my_head": {}
        },
        "worlds": {
          "over_my_head_dev_world": {}
        }
      }
    }
  }
}